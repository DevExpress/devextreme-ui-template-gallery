{
  "name": "devextreme-ui-template-gallery",
  "private": true,
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "set-theme": "node utils/set-theme.js",
    "copy-build": "node utils/copy-build.js",
    "copy-shell": "node utils/copy-shell.js",
    "make-embedded-mode": "node utils/make-embedded-mode.js",
    "lint-html": "prettier --write . && prettier --check .",
    "lint-js": "eslint .",
    "lint-fix": "prettier --write . && eslint --fix . && stylelint **/*.{css,scss} --fix",
    "lint-css": "stylelint **/*.{css,scss}",
    "lint-css-fix": "stylelint --fix **/*.{css,scss}",
    "lint-angular": "eslint packages/angular",
    "lint-react": "eslint packages/react",
    "lint-vue": "cd packages/vue && vue-cli-service lint",
    "lint-utils": "eslint utils/",
    "lint-bundles": "npm-run-all -p lint-angular lint-react lint-vue",
    "lint-angular-fix": "eslint --fix packages/angular",
    "lint-react-fix": "eslint --fix packages/react",
    "lint-utils --fix": "eslint --fix utils"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@typescript-eslint/utils": "^8.46.2",
    "devextreme-cli": "latest",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-perf": "^3.3.1",
    "eslint-plugin-spellcheck": "^0.0.20",
    "fs-extra": "^10.0.0",
    "globals": "^16.5.0",
    "prettier": "2.4.1",
    "stylelint": "^15.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-scss": "^5.3.2",
    "vue": "^3.2.38",
    "vue-eslint-parser": "^9.3.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "devextreme": "25.1.7",
    "devextreme-dist": "25.1.7",
    "devextreme-themebuilder": "25.1.7",
    "eslint-config-devextreme": "1.1.6",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2"
  },
  "pre-commit": [
    "lint-bundles"
  ],
  "overrides": {
    "axios@<0.30.2": "~0.30.2",
    "follow-redirects@<1.15.6": "~1.15.6",
    "glob@<10.5.0": "~10.5.0",
    "nth-check@<2.0.1": "~2.0.1",
    "@angular/animations": "~20.3.15",
    "@angular/common": "~20.3.15",
    "@angular/compiler": "~20.3.15",
    "@angular/compiler-cli": "~20.3.15",
    "@angular/core": "~20.3.15",
    "@angular/forms": "~20.3.15",
    "@angular/platform-browser": "~20.3.15",
    "@angular/platform-browser-dynamic": "~20.3.15",
    "@angular/router": "~20.3.15",
    "@angular/cli": "~20.3.5",
    "@angular-devkit/build-angular": "~20.3.13",
    "@angular-devkit/core": "~20.3.13",
    "@angular-devkit/schematics": "~20.3.13",
    "@angular-eslint/builder": "~20.1.1",
    "@angular-eslint/eslint-plugin": "~20.1.1",
    "@angular-eslint/eslint-plugin-template": "~20.1.1",
    "@angular-eslint/schematics": "~20.1.1",
    "@angular-eslint/template-parser": "~20.1.1",
    "@angular/build": "~20.3.5",
    "zone.js": "~0.15.1"
  }
}
